# ----------------------------------------------------------------------------------------------------------------------
# SpringTurbo
# ----------------------------------------------------------------------------------------------------------------------
spring.turbo.convention.ModulesConvention=\
spring.turbo.convention.spi.ModulesConventionImpl
spring.turbo.convention.ExtraMessageSourceBasenameConvention=\
spring.turbo.convention.spi.ExtraMessageSourceBasenameConventionImpl
# ----------------------------------------------------------------------------------------------------------------------
# Spring
# ----------------------------------------------------------------------------------------------------------------------
org.springframework.boot.env.PropertySourceLoader=\
spring.turbo.util.propertysource.HoconPropertySourceLoader,\
spring.turbo.util.propertysource.TomlPropertySourceLoader
org.springframework.boot.env.EnvironmentPostProcessor=\
spring.turbo.core.SpringApplicationHoldersEnvironmentPostProcessor,\
spring.turbo.core.env.MiscellaneousSetupEnvironmentPostProcessor,\
spring.turbo.core.env.LoadmeEnvironmentPostProcessor
org.springframework.context.ApplicationListener=\
spring.turbo.core.SpringApplicationHolders